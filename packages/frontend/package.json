{
  "name": "@dsconstruction/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "npx eslint -- .",
    "deploy": "node ./scripts/deploy.js",
    "destroy": "node ./scripts/destroy.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "mui-datatables": "^2.0.0-beta.59",
    "notistack": "^0.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-cookie": "^3.1.1",
    "react-dom": "^16.8.5",
    "react-dropzone": "^10.1.0",
    "react-google-recaptcha-v3": "^1.1.0",
    "react-helmet": "^5.2.0",
    "react-modal-hook": "^1.1.0",
    "react-request-hook": "^1.0.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-use-form-state": "^0.9.0",
    "styled-components": "^4.2.0",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@tomfischer/eslint-config": "^1.0.6",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "minimist": "^1.2.0",
    "prettier": "^1.16.4",
    "serverless": "^1.40.0",
    "serverless-build-client": "^2.1.4",
    "serverless-finch": "^2.4.2",
    "shelljs": "^0.8.3"
  },
  "eslintConfig": {
    "extends": [
      "@tomfischer/eslint-config",
      "react-app"
    ],
    "plugins": [
      "import",
      "flowtype",
      "jsx-a11y",
      "react"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-sequences": "off",
      "no-unused-vars": "error",
      "react/prop-types": "error",
      "prettier/prettier": "error"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
